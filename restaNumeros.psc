Algoritmo restaNumeros
	Definir num1, num2, result Como Entero;
	Escribir "Haz ingresado a la suma de numeros...";
	Escribir "Ingrese el primer numero a restar: ";
	Leer num1;
	Escribir "Ingrese el segundo numero a restar: ";
	Leer num2;
	result <- num1 * num2;
	Escribir "El resultado de la resta es: " ,result;
	
FinAlgoritmo
